<% if flash[:errors]%>
<% flash[:errors].each do |e| %>
  <%= e %>
  <% end %>
<% end %>

<%= form_for @duck do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :description %><br>
<%= f.text_field :description %><br><br>
<%= f.collection_select :student_id, @students, :id, :name %>
<br>
<%= f.submit %>
<% end %>

